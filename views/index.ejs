<%- include('partials/header') -%>
<% foundBlogs.forEach((blog) => { %>
		<h1><%= blog.title %></h1>
		<img src=" <%= blog.image %> "> 
		<p> <%= blog.content %> </p>
		<span><%= blog.dateCreated.toDateString() %></span>
		<a href="/posts/<%= blog._id %>">Read More</a>
<% }) %>
<%- include('partials/footer') -%>
